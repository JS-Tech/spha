console.log("<%= @output %>");
$("#flash").html('<%= j render partial: "layouts/flash" %>');
$("#output-" + "<%= @site %>").html('<%= j(render partial: "output", locals: { output: @res[0], status: @res[1] } ) %>');

<% if @exit_code != 0 %>
  $("#output-" + "<%= @site %> pre").addClass("console-error");
<% end %>
